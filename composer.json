{
    "name": "nullref/yii2-eav",
    "type": "yii2-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "kartik-v/yii2-widget-select2": "^2.1@dev",
        "marciocamello/yii2-x-editable": "dev-master",
        "nullref/yii2-core": ">=0.0.4",
        "nullref/yii2-useful": ">=0.0.4",
        "unclead/yii2-multiple-input": "~2.0",
        "yiisoft/yii2": ">=2.0.13"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": {
            "nullref\\eav\\": "src/"
        }
    },
    "extra": {
        "bootstrap":"nullref\\eav\\Bootstrap"
    }
}
